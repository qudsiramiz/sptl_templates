  title     = {A {TURBULENCE}-{DRIVEN} {MODEL} {FOR} {HEATING} {AND} {ACCELERATION} {OF} {THE} {FAST} {WIND} {IN} {CORONAL} {HOLES}},
  year      = {2009},
