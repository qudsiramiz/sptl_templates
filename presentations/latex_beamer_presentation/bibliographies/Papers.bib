@Article{Petrinec2022,
  author   = {Petrinec, S.M. and Burch, J.L. and Fuselier, S.A. and Trattner, K.J. and Giles, B. and Strangeway, R.J.},
  journal  = {Journal of Geophysical Research: Space Physics},
  title    = {On the Occurrence of Magnetic Reconnection Along the Terrestrial Magnetopause, Using Magnetospheric Multiscale (MMS) Observations in Proximity to the Reconnection Site},
  note     = {e2021JA029669 2021JA029669},
  number   = {n/a},
  pages    = {e2021JA029669},
  volume   = {n/a},
  abstract = {Abstract Observations at the reconnection site from the MMS mission are used to examine the occurrence of magnetic reconnection along the magnetopause in relation to both the upstream IMF orientation and local magnetosheath variations. While there is a statistically larger number of reconnection intervals observed downstream of the quasi-parallel bow shock than is accounted for based on the histogram of overall IMF orientations, it is surmised that increased fluctuations downstream of the quasi-parallel bow shock cause greater large-scale motion of the magnetopause surface; but do not preferentially enhance the occurrence of steady magnetic reconnection. An examination of the locations of observed reconnection sites relative to model predictions also suggest that enhanced magnetosheath fluctuations do not result in steady reconnection occurring at random locations at the magnetopause. This article is protected by copyright. All rights reserved.},
  doi      = {https://doi.org/10.1029/2021JA029669},
  eprint   = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2021JA029669},
  file     = {:Research/Papers/Petrinec2022:},
  keywords = {Magnetopause, Reconnection, Magentosheath, Bow shock},
  url      = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2021JA029669},
}

@Article{Trenchi2008,
  author   = {Trenchi, L. and Marcucci, M. F. and Pallocchia, G. and Consolini, G. and Bavassano Cattaneo, M. B. and di Lellis, A. M. and R{\`e}Me, H. and Kistler, L. and Carr, C. M. and Cao, J. B.},
  journal  = {Journal of Geophysical Research (Space Physics)},
  title    = {Occurrence of reconnection jets at the dayside magnetopause: Double Star observations},
  year     = {2008},
  month    = jul,
  number   = {A7},
  pages    = {A07S10},
  volume   = {113},
  abstract = {We present a statistical study on reconnection occurrence at the dayside         magnetopause performed using the Double Star TC1 plasma and         magnetic field data. We examined the magnetopause crossings that         occurred during the first year of the mission in the 0600-1800         LT interval and we identified plasma flows, at the magnetopause         or in the boundary layer, with a different velocity with respect         to the adjacent magnetosheath. We used the Wal{\'e}n relation to         test which of these flows could be generated by magnetic         reconnection. For some event we observed opposite-directed         reconnection jets, which could be associated with the passage of         the X-line near the satellite. We analyzed the occurrence of the         reconnection jets and reconnection jet reversals in relation to         the magnetosheath parameters, in particular the local Alfv{\`e}n         Mach number, the plasma {\ensuremath{\beta}}, and the magnetic         shear angle. We also studied the positions and velocities of the         reconnection jets and jet reversals in relation to the         magnetosheath magnetic field clock angle. We found that the         observations indicate the presence of a reconnection line hinged         near the subsolar point and tilted according to the observed         magnetosheath clock angle, consistently with the component         merging model.},
  doi      = {10.1029/2007JA012774},
  eid      = {A07S10},
  file     = {:Research/Papers/Trenchi2008.pdf:PDF},
  keywords = {Radio Science: Magnetospheric physics (2700), Solar Physics, Astrophysics, and Astronomy: Magnetic reconnection (2723, 7835), Space Plasma Physics: Magnetic reconnection (2723, 7526), Magnetospheric Physics: Magnetosheath, reconnection, magnetopause},
  url      = {https://ui.adsabs.harvard.edu/abs/2008JGRA..113.7S10T},
}

